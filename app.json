{
  "expo": {
    "scheme": "parkme",
    "name": "ParkMe",
    "slug": "parkme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to find nearby parking spots.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to provide parking notifications and find nearby spots.",
        "UIBackgroundModes": [
          "location",
          "remote-notification"
        ],
        "LSApplicationQueriesSchemes": ["googlechromes", "comgooglemaps"]
      },
      "config": {
        "googleMapsApiKey": "googleMapsApiKey"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "googleMapsApiKey"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow ParkMe to access your camera"
        }
      ],
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#0066cc"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ebc5f21d-48e0-4eaf-a818-9e2a09456fd0"
      }
    },
    "owner": "w4n73d"
  }
}